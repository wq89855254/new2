.container
        height 100%
        background #fff
        .tb
            thead
                background: blue
        .head
            display flex
            justify-content space-between
            height 64px
            line-height 64px
            .imgWrap
                .save_img
                    height 25px
                    vertical-align middle 
            .back
                font-size 13px
                margin-right 30px
                img
                    height 21px
                    vertical-align middle
                    cursor pointer
                span 
                    cursor pointer
        .saveFile
            width 1488px
            height 293px
            margin 0 auto
            border-radius 15px
            background #f7f7f7
            box-shadow 1px 1px 3px 1px #d0d0d0
            .form
                .wraper
                    position relative
                    height 47px
                    line-height 47px
                    padding-left 20px
                    &:not(:last-child)::before
                        position absolute
                        top 100%
                        left 85px
                        width 90%
                        height 1px
                        content ''
                        background #e9e9e9
                    label 
                        font-size 13px
                        color #43578c
                        margin-right 10px
                .data
                    label:nth-of-type(2)
                        margin-left 10px
                .productType
                    .choose
                        height 26px
                        line-height 26px
                        text-align center
                        padding 3px 7px
                        font-size 12px
                        border-radius 3px
                        border 1px solid #2275db
                        background linear-gradient(#72b6fd,#3a90e7)
                        color #fff
                        cursor pointer

                .weatherType
                    display flex
                    .typeList
                        display flex
                        align-items center
                        margin-bottom 0
                        li
                            height 26px
                            line-height 26px
                            padding 0 11px
                            margin-right 7px
                            border 1px solid #dde8f7
                            border-radius 5px
                            background:linear-gradient(#edf1fc,#dbe7f7)
                            font-size 13px
                            color #7f807b
                            input
                                vertical-align middle
                .weatherSystem
                    display flex
                    .systemList
                        display flex
                        align-items center
                        margin-bottom 0

                        li
                            height 26px
                            line-height 26px
                            padding 0 11px
                            margin-right 7px
                            border 1px solid #dde8f7
                            border-radius 5px
                            background:linear-gradient(#edf1fc,#dbe7f7)
                            font-size 13px
                            color #7f807b
                            input
                                vertical-align middle
                            &:last-child
                                border none
                                background:#f7f7f7
                                

                .explain
                    input 
                        width 60%
                        height 39px
                        border 1px solid #d0d0d0
                        padding-left 5px
            .save
                width 91px
                height 28px
                line-height 28px
                text-align center
                margin 14px auto
                cursor pointer
                border-radius 16px
                background linear-gradient(#64b1f9,#3492ea)
                color #ffffff

        .historyFile
            width 1488px
            margin 0 auto
            .history_head
                height 50px
                line-height 70px
                text-align center
                font-size 12px
                color #6c6c6c
            .table
                width 100%
                border 1px solid #e7e7e7
               
                thead
                    th
                        border-bottom 1px solid #e7e7e7
                        text-align left
                        height 30px
                        line-height 30px
                        color #2b4c6f
                        font-size 14px
                        background linear-gradient(#d7e7f7,#d6e6f6)
                        &:not(.center)
                            padding-left 30px
                        &.center
                            text-align center

                tbody
                    td  
                        border-bottom 1px solid #e7e7e7
                        height 34px  
                        line-height 34px
                        text-align left
                        color #575757
                        font-size 12px
                        overflow hidden
                        &:not(.center)
                            padding-left 30px
                        &.center
                            text-align center
                        &.lookDetail
                            color #556386
                            span 
                                display inline-block
                                border 1px solid
                                height 24px
                                line-height 24px
                                border 1px solid #c6cddf
                                border-radius 5px
                                padding 0 10px
                                background linear-gradient(#f3f3f5,#deebf1)
        .pages
            width 190px
            margin 20px auto 0
            color #606165
            font-size 13px
            .prePage,.nextPage
                font-size 14px
                color #c2d2e1
                margin-right 30px
                cursor pointer
            .toPage
                display:inline-block
                width 45px
                height 28px
                line-height 28px
                border 1px solid #dcdfe6
                border-radius 5px
                margin 0 5px
                text-align center
        
        
        .checkProduct
            // display none
            position absolute
            left 50%
            top 50%
            transform: translate(-50%,-50%)
            z-index 5
            
            // width 1290px
            // width auto 
            height 570px
            padding 0 15px
            background-color #eff2f5
            border-radius 5px
            .title
                height 46px
                line-height 46px
                text-align center
                boder 1px solid 
                font-size 14px
                color #3d3d3d
            .productList
                display flex
                color #303030
                font-size 14px
                li
                    min-width 220px
                    padding 16px 20px 0 8px
                    height 429px
                    overflow auto
                    border-radius 5px
                    border 1px solid #dedede
                    margin-left 10px
                    background-color #fff
                    input[type='checkbox']
                        -moz-appearance: none;
                        width: 11px;
                        border-radius: 2px;
                        height: 11px;
                        background-color: transparent;
                        border: 1px solid #1BBA79;
                        


                    div
                        margin 5px 0
                        .second_level
                            div
                                padding-left 15px
                        
                        .product_type
                            margin 0 5px
                        img
                            margin-top -3px
                            vertical-align middle
            .btn_group
                width 213px
                margin 20px auto
                span
                    display inline-block
                    width 90px
                    text-align center
                    height 30px
                    line-height 30px
                    border-radius 20px
                    font-weight bold
                    cursor pointer
                    &.confirm
                        color #fff
                        background linear-gradient(#63b0f9,#3591eb)
                    &.cancel
                        background-color #e3e3e3
                        color #b8b8b8
                        margin-left 10px
            .close
                position absolute
                right -40px
                top 10px
        .shade
            // display:none
            position absolute
            top 0
            left 0
            width 100%
            height 100%
            background rgba(0,0,0,.6)
            z-index 2