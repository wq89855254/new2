.container
    width 100%
    height 100%
    background #fff
    .head
        display flex
        justify-content space-between
        height 64px
        line-height 64px
      
        .back
            font-size 13px
            margin-right 30px
            img
                vertical-align middle
                cursor pointer
            span 
                cursor pointer
    .center_wrap
        height 778px
        display flex
        justify-content: space-between
        width 1480px
        margin 0 auto
        .accordion
            border: 1px solid #9dc8f3
            width 407px
            height: 100%
            border-radius: 5px
            :global(.ant-collapse-content)
                height 493px
                font-weight: normal
                overflow: auto
            &>div
                // min-height 33px
                line-height 33px
                color #7e817f
                &>h2
                    height 33px
                    margin-bottom:0
                    color rgb( 62, 62, 62 )
                    font-size 15px
                    background linear-gradient(to bottom,#eaf3fe 10%,#cadffe 100%)
                    &.active
                        color #0049af
                &>.year_list 
                    margin-bottom 0
              
                    &>li
                        padding-left 10px
                        .isOPen
                            display inline-block
                            width 11px
                            height 11px
                            line-height 7px
                            text-align center
                            border 1px solid #7e817f
                            font-size: 10px
                img
                    float: right
                    margin-top 10px
                    margin-right 10px
                    cursor pointer
        .pdf_show
            position relative
            box-sizing border-box
            width 1042px
            height: 100%
            overflow: auto
            
            padding-bottom: 30px
            background #525659
            .cat_list
                display flex
                justify-content: space-between
                padding 0 25px
                color #fff
                height 49px
                line-height 49px
                font-size 14px
                background #323639
            .img_wrap
                width 793px
                height 100%
                background #fff
                margin 0 auto
            // 右侧功能列表
            .button_group
                position absolute
                right 20px
                bottom 150px
                width 40px
                &>div
                    width 40px
                    height 40px
                    border-radius 50%
                    background #fff
                    line-height 40px
                    font-size 16px
                    text-align: center
                    color #7b7b7b
                    cursor pointer
                    &:nth-child(2)
                        margin 15px 0 5px