.container
        height 100% 
        background-image linear-gradient(to right,#d9e4f6,#dee7f8)
        overflow hidden
        &>.head_title
            padding 15px 0 10px 50px
            overflow hidden
            .left_title
                float left
                margin-left 110px
                width 468px
            .right_title
                margin-left: 30px
                width 1096px
                height 42.6px
                line-height 42.6px
                float left
                .product_list
                    float left
                    li
                        float left
                        font-size 12px
                        color: rgb( 44, 76, 112 )
                        &.filt
                            cursor pointer
                            span    
                                display inline-block
                                width 81px
                                height 24px
                                margin 0 15px
                                line-height 24px 
                                text-align center
                                border-color: rgb( 98, 171, 246 );
                                border-radius: 12px;
                                color: rgb( 255, 255, 255 );
                                text-shadow: 0.5px 0.866px 2px rgb( 80, 138, 195 )
                                background-image:linear-gradient( 90deg, rgb(53,145,235) 0%, rgb(101,177,250) 100%);
                                img
                                    vertical-align middle
                .date_list
                    float right
                    li
                        float left
                        margin-left 10px
                        font-size 12px
                        color #515151
                        &.area
                            select
                                width 56px
                                height 23px
                                line-height 23px
                                border-radius 3px
                                border 1px solid #7b9cc2
                                font-size 11px
        .centerWrap
            width 1568px
            margin 0 auto
            .history_search 
                float left
                box-sizing border-box
                padding 0 9px
                width 480px
                .weather_info_search
                    border 1px solid #a6c1d6
                    border-radius 3px
                    background-image url('./imgs/search_top.png'),url('./imgs/search_foot.png')
                    background-repeat no-repeat
                    background-position top,bottom
                    background-color #fff

                    input 
                        vertical-align middle
                    .w_title
                        height 41px
                        line-height 40px
                        text-align center
                        font-size 14px
                        font-weight 500
                        vertical-align middle
                        img
                            vertical-align middle
                            height 28px
                        .checkall
                            color #7f7f7f
                            font-size 14px
                    .date_search
                        .date_group
                            display flex
                            justify-content space-around
                            .begintime,.endtime
                                display flex
                                label 
                                    display inline-block
                                    font-size 12px
                                    line-height 25px
                                    vertical-align middle
                                    color #40598b
                                    margin-right 3px
                                input 
                                    width  97px
                                    height 20px
                                    margin-right 3px
                                select
                                    height 18px
                                    font-size 12px
                    // 天气类型
                    .weather_type
                        
                        .weatherType_list
                            display flex
                            width 90%
                            margin 0 auto
                            margin-bottom 0
                            li
                                border 1px solid #ccdcf3   
                                padding 0 4px
                                height 26px
                                line-height 26px
                                font-size 13px
                                color #737172
                                border-radius 5px
                                background linear-gradient(#f0f5fb 10%,#dbe7f7)
                                
                                &.active
                                    color #ffffff
                                    background linear-gradient(#87b3f0 10%,#4c8deb)
                            
                    // 天气系统
                    .weather_system
                        .w_title
                            padding-left 62px
                        .systemList
                            width 90%
                            margin 0 auto
                            color #1958b5
                            display flex
                            flex-wrap: wrap
                            justify-content: space-around
                            // padding-left 15px  
                            cursor pointer
                    // 选择地区    
                    .region_check
                        .w_title
                            padding-left 60px
                        .region_list
                            width 90%
                            margin 0 auto
                            display flex
                            flex-wrap wrap
                            :global(label)
                                color #737373
                                font-size 14px
                                margin-right 20px
                                margin-bottom 5px
                                img 
                                    vertical-align middle       
                    // 查询按钮                     
                    .seach_info
                        width 84px
                        height 28px
                        text-align center
                        line-height 28px
                        margin 15px auto
                        color #fff
                        border-radius 20px
                        background linear-gradient(#63abf7,#3692e9)
                        cursor pointer

                // 查询结果
                .weather_info_result
                    border 1px solid #a6c1d6
                    border-radius 3px
                    margin-top 13px
                    background #fff
                    padding 0 8px
                    .result_title
                        display flex
                        justify-content space-between
                        height 39px
                        line-height 39px
                        color #57607f
                        h2
                            padding 0 0 0 87px
                            font-size 14px
                        .click_check
                            color #7f7f7f
                            font-size small
                    .table
                        display block
                        width 100%
                        max-height 300px;
                        text-align left
                        overflow auto
                                            
                        &::-webkit-scrollbar {/*滚动条整体样式*/
                            width: 6px;     /*高宽分别对应横竖滚动条的尺寸*/
                            height: 6px;
                        }
                        &::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
                            border-radius: 5px;
                            -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
                            background: #85ade1;
                        }
                        &::-webkit-scrollbar-track {/*滚动条里面轨道*/
                            -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
                            border-radius: 0;
                            background: rgba(0,0,0,0.1);
                        }

                        thead
                            display block
                            width 100%
                            height 36px
                            line-height 36px
                            font-size 13px
                            background-color #d7e7f7
                        
                            th
                                padding-left 11px
                                &.type_head
                                    padding-left 67px
                                &.explain_head
                                    padding-left 67px

                        tbody
                            position relative
                            display block
                            height 66px
                            border 1px solid #e1e1e1
                            margin-top 6px
                            &::after
                                position absolute
                                top 0
                                left 0
                                content:''
                                display inline-block
                                width 100%
                                height 100%
                                background rgba(0,0,0,0)
                            &.active
                                border-radius: 2px;
                                background-image:linear-gradient( 90deg, rgb(79,114,205) 0%, rgb(68,105,191) 20%, rgb(56,96,177) 50%, rgb(72,107,188) 85%, rgb(87,118,199) 100%);
                                td
                                    color #fff
                                    &.center
                                        .num
                                            background-color: rgb( 71, 116, 213 );
                                            color #fff

                            td
                                display table-cell
                                vertical-align middle
                                height 28px
                                font-size 13px
                                padding-left 10px
                                &.center
                                    .num 
                                        display block
                                        width: 24px;
                                        height: 24px
                                        text-align center
                                        font-size: 12px;
                                        font-family: "Microsoft YaHei";
                                        color: rgb( 82, 82, 82 )
                                        line-height 24px
                                        border-radius 50%
                                        background-color: rgb( 215, 234, 255 )
            .product_show
                position relative
                float left
                width 1088px
                .pre,.next
                    position absolute
                    top 50%
                    transform translateY(-50%)
                    cursor: pointer
                .pre
                    left 20px
                .next
                    right 20px
                &>img
                    width 100%
        .shade
            position absolute
            top 0
            left 0
            width 100%
            height 100%
            background rgba(0,0,0,.6)
            z-index 2


        .checkProduct
            position absolute
            top 50%
            left 50%
            transform: translate(-50%,-50%)
            
            z-index 5
            // width 1290px
            // width auto 
            padding 10px 15px 0
            background-color #eff2f5
            border-radius 5px
            .title
                height 46px
                line-height 46px
                text-align center
                boder 1px solid 
                font-size 14px
                color #3d3d3d
            .productList
                display flex
                color #303030
                font-size 14px
                &>li
                    min-width: 300px
                    height: 500px
                    overflow auto
                    padding 16px 12px
                    border-radius 5px
                    border 1px solid #dedede
                    margin-left 10px
                    background-color #fff
                    font-size 14px
                    color: rgb( 86, 86, 86 )
                

                    &>.level_cont
                        border-bottom 1px solid #e8e8e8
                        :global(.ant-tree li)
                            display none
                        :global(.ant-tree-checkbox)  
                            display none
                        :global(.ant-tree li.ant-tree-treenode-checkbox-indeterminate)   
                            display block
                        :global(.ant-tree li.ant-tree-treenode-checkbox-checked)
                            position relative
                            display block
                        :global(.ant-tree li .ant-tree-node-content-wrapper.ant-tree-node-content-wrapper-normal)
                            position relative
                            :after
                                position absolute
                                right -20px
                                top 3px
                                content: ''
                                display inline-block
                                width 18px
                                height 18px
                                background-image: url('./imgs/circle_blank.png')
                        :global(.ant-tree li .ant-tree-node-content-wrapper.ant-tree-node-content-wrapper-normal.ant-tree-node-selected)
                            :after
                                background-image: url('./imgs/circle_active.png')

                    &.child_choose
                        li
                            padding-left 10px
                            width: 208px;
                            height: 23px
                            line-height 23px
                            &.active
                                background-color: rgb( 212, 233, 253 );
                    &.picture_choose
                        li
                            padding-left 10px
                            width: 208px;
                            height: 23px
                            line-height 23px
                            &.active
                                background-color: rgb( 212, 233, 253 );
            .btn_group
                width 213px
                margin 20px auto
                span
                    display inline-block
                    width 90px
                    text-align center
                    height 30px
                    line-height 30px
                    border-radius 20px
                    font-weight bold
                    &.confirm
                        color #fff
                        background linear-gradient(#63b0f9,#3591eb)
                        cursor pointer
                    &.cancel
                        background-color #e3e3e3
                        color #b8b8b8
                        margin-left 10px
                        cursor pointer
            .close
                position absolute
                right -40px
                top 10px
                cursor pointer