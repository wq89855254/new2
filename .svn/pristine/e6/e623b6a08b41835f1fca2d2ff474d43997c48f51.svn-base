.box {
    height: 60px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0 10px;
    >div {
        // width: 100%;
    }
    >div:first-child {
        margin-bottom: 5px;
    }
    >div:nth-child(2) {
        display: flex;
        justify-content: space-between;
    }
}